ses="${ADMIN_SESSIONS}"
if test "${ses}" = "null"; then ses="3"; fi

. /usr/local/bin/get_py_log

cd ${BASE}/python/admin
exec gunicorn \
        --workers ${ses} \
        --user=daemon \
		${extra} --bind unix:/run/wsgi_admin.sock \
        wsgi >/tmp/adminui.log 2>&1
