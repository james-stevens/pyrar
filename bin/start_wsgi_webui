ses="$(jq .webui_sessions ${BASE}/etc/policy.json | tr -d '"')"
if test "${ses}" = "null"; then ses="3"; fi

. /usr/local/bin/get_py_log

cd ${BASE}/python/webui
exec gunicorn \
        --workers ${ses} \
		${extra} --bind unix:/run/wsgi_checker.sock \
        wsgi >/tmp/start_wsgi.log 2>&1
