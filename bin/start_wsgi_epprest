# Alternative license arrangements are possible, contact me for more information

session="${1}"
provider="${2}"

ses="$(jq .${provider}.sessions ${BASE}/etc/providers.json)"
if test "${ses}" = "null"; then ses="3"; fi

export PYRAR_PROVIDER="${provider}"

. /usr/local/bin/get_py_log

cd ${BASE}/python/epprest
exec gunicorn \
        --workers ${ses} \
        ${extra} --bind unix:/run/epp/wsgi_epprest_${provider}_${session}.sock \
        wsgi >/tmp/start_wsgi.${provider}_${session}.log 2>&1
